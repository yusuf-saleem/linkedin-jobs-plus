{
  "manifest_version": 3,
  "name": "Jobs+",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://www.linkedin.com/*"
      ],
      "js": ["content.js"]
    }
  ]
}
